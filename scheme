# Moire scheme file

: tag-constraint

list:item,list,ordered
ordered:item,list,ordered



: formats



:: html

name:"HTML"
extension:"html"

::: tags

begin 0:<html><link rel = "stylesheet" href = "style.css"><body>
end 0:</body></html>
0 1:<title>\1</title>
1 1:<h1>\1</h1>
2 1:<h2>\1</h2>
3 1:<h3>\1</h3>
4 1:<h4>\1</h4>
5 1:<h5>\1</h5>
6 1:<h6>\1</h6>
b 1:<b>\1</b>
code 1:<pre><tt>\c1</tt></pre>
code 2:<pre><tt>\c1</tt></pre>{warning("code language skipped");}
href 1:<a href = "\1">\1</a>
href 2:<a href = "\1">\2</a>
i 1:<i>\1</i>
image 1:<img src = "\1"/>
image 2:<img src = "\1" alt = "\2"/>
item 1:<li>\1</li>
list 1:<ul>\1</ul>
ordered 1:<ol>\1</ol>
size 2:<span sytle = "font-size = \1">\2</span>
strike 1:<s>\1</s>
sub 1:<sub>\1</sub>
sup 1:<sup>\1</sup>
table 1:<table>\1</table>
td 1:<td>\1</td>
tr 1:<tr>\1</tr>
